<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="bulk-offline-update-screens-service"
        rep:privileges="{Name}[jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/content/screens/*"/>
    </allow>
    <allow1
        jcr:primaryType="rep:GrantACE"
        rep:principalName="author-publish-screens-service"
        rep:privileges="{Name}[jcr:read,jcr:modifyAccessControl,jcr:readAccessControl]"/>
    <allow2
        jcr:primaryType="rep:GrantACE"
        rep:principalName="screens-group-admin-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write,jcr:lockManagement,rep:userManagement]"/>
    <allow3
        jcr:primaryType="rep:GrantACE"
        rep:principalName="screens-we-retail-devices"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/content/screens/we-retail"/>
    </allow3>
    <allow4
        jcr:primaryType="rep:GrantACE"
        rep:principalName="screens-we-retail-administrators"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/content/screens/we-retail"/>
    </allow4>
    <allow5
        jcr:primaryType="rep:GrantACE"
        rep:principalName="pageexporterservice"
        rep:privileges="{Name}[jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write]"/>
    <allow6
        jcr:primaryType="rep:GrantACE"
        rep:principalName="contentsync-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write,jcr:lockManagement]"/>
    <allow7
        jcr:primaryType="rep:GrantACE"
        rep:principalName="translation-job-service"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,jcr:modifyAccessControl,jcr:readAccessControl,rep:write,jcr:lockManagement]"/>
</jcr:root>
